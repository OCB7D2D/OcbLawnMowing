<configs patcher-version="4">

  <append xpath="/items">

    <item name="ocbElectricityModernWindmillBlueprint">
      <property name="Extends" value="schematicNoQualityMaster"/>
      <property name="CustomIcon" value="ui_game_symbol_windmill_modern"/>
      <property name="Group" value="Schematics,BPScience,Books,Items"/>
      <effect_group tiered="false">
        <triggered_effect trigger="onSelfPrimaryActionEnd" action="ModifyCVar" cvar="ocbElectricityModernWindmill" operation="set" value="1"/>
        <triggered_effect trigger="onSelfPrimaryActionEnd" action="GiveExp" exp="50"/>
      </effect_group>
    </item>
  </append>

  <modif condition="UndeadLegacy_CoreModule">
    <include path="items.a20.xml"/>
    <include path="items.ulm.xml"/>
  </modif>
  <modelsif condition="Darkness Falls Core">
		<include path="items.a20.xml"/>
		<include path="items.df.xml"/>
	</modelsif>
	<modelse>
		<include path="items.a20.xml"/>
	</modelse>
	
</configs>
